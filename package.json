{
    "name": "vcode-cell-language",
    "displayName": "Cell",
    "description": "Support extension for Cell language",
    "version": "0.0.1",
    "publisher": "cntdwn",
    "author": "Ernesto Giralt <egiralt@gmail.com>",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:egiralt/vscode-language-cell.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "cell",
                "aliases": [
                    "Cell",
                    "cell"
                ],
                "extensions": [
                    ".cell"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cell",
                "scopeName": "source.assembly.cell",
                "path": "./syntaxes/cell.tmLanguage.json"
            }
        ]
    }
}